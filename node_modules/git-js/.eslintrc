{
    "extends": "eslint:recommended",
    "env": {
        "es6": true,
        "mocha": true,
        "node": true
    },
    "globals": {
        "expect": true
    },
    "rules": {
        "eol-last": 2,
        "indent": ["error", 4],
        "no-console": 0,
        "no-param-reassign": 0
    }
}
